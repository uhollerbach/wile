(define x 1)
(define t 2)

(define (addit y) (+ x y))

(define (dn msg v)
  (write-string msg)
  (write-string ": ")
  (display v)
  (newline))

(dn "x" x)
(dn "+5" (addit 5))
(newline)
(let ((x 10))
  (dn "x" x)
  (dn "+5" (addit 5)))
(newline)
(dn "x" x)
(dn "+5" (addit 5))
(newline)
(fluid-let ((x 10)
	    (t 14))
  (dn "X" x)
  (dn "?5" (addit 5)))
(newline)
(dn "x" x)
(dn "+5" (addit 5))
